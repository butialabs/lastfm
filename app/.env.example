# URL da aplicação (usado em redirects OAuth)
APP_URL=http://localhost:8080

# Banco de Dados
DB_CONNECTION=sqlite
SQLITE_PATH=data/db/lastfm.sqlite

# Last.fm API
LASTFM_API=your_lastfm_api_key

# MusicBrainz como fonte principal de imagens (opcional)
# Se true, busca primeiro no MusicBrainz e usa Last.fm como fallback
# Se false ou não definido, usa Last.fm como principal e MusicBrainz como fallback
MUSICBRAINZ_DEFAULT=false

# Segurança (32 chars recomendados)
ENCRYPTION_KEY=your_32_char_encryption_key_123456

# Admin Panel Credentials
ADMIN_USER=admin
ADMIN_PASSWORD=change_this_password